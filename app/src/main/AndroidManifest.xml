<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yyyu.mdfeaturetestdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".bottom_navigation.TestBottomNavActivity"
            android:label="Bottom Navigation "/>
        <activity android:name=".animation.TestAniActivity"
            android:label="Animatio "/>
        <activity android:name=".transition.TestActTransitionActivity"
            android:label="transition动画测试"/>
